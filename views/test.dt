doctype html
- import std.string : toUpper;
- string title = "Embedded D code example";
    head
        title= title.toUpper
    body
        h1 #{ title }
        - foreach(i; 1..4)
            p This is paragraph #{i}
        
        - int max(int a, int b)
            - if (a > b) return a; else return b;
        
        p The maximum of 5 and 12 is #{ max(5, 12) }.